    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0"
  },
  "scripts": {
    "start": "nodemon --watch \"./backend/\" -e \"*\" -i \"config.json\" --trace-deprecation --trace-uncaught --max-old-space-size=8192 --trace-warnings --unhandled-rejections=strict backend/index.js",
    "production": "npm run compress && node --trace-deprecation --trace-uncaught --max-old-space-size=8192 --trace-warnings --unhandled-rejections=strict dist/index.js --production",
    "build": "cd frontend && next build",
    "lint": "prettier --write .",
    "dev-electron": "electron ./electron",
    "postinstall": "node postinstall.js",
    "types": "node installTypes.js",
    "compress": "node compress.js"
  },
  "engines": {
    "node": ">= 14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freedom-app/freedom.git"
  },
  "keywords": [
    "socket.io",
    "react",
    "next.js"
  ],
