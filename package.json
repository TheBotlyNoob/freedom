    "jsdom": "^17.0.0",
    "marked": "^3.0.3",
    "mongoose": "^6.0.5",
    "nan": "^2.15.0",
    "nanoid": "^3.1.25",
    "next": "^11.1.2",
    "nodemon": "^2.0.12",
    "peer": "^0.6.1",
    "peerjs": "^1.3.2",
    "prettier": "^2.4.0",
    "prompts": "^2.4.1",
    "prop-types": "^15.7.2",
