    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "postcss": "^8.3.6",
    "prettier-eslint": "^13.0.0",
    "tailwindcss": "^2.2.15"
